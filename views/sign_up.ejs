                </section>
                <input name="user_id" id="user_id" autocomplete="off" required placeholder="ID" class="form-control input-lg" />
                <input type="password" id="password" name="password" required placeholder="PASSWORD" autocomplete="new-password" class="form-control input-lg" />
                <input type="password" id="password_confirm" name="password_confirm" required placeholder="PASSWORD CONFIRM" autocomplete="new-password" class="form-control input-lg" />
                <input type="text" id="user_name" name="user_name" required placeholder="NAME" class="form-control input-lg" />
                <input type="text" id="phone" name="phone" required placeholder="PHONE" class="form-control input-lg" />
                <input type="text" id="student_id" name="student_id" required placeholder="STUDENT ID" class="form-control input-lg" />
                <button type="submit" name="go" class="btn btn-block btn-info">CONFIRM</button>

                <section>
                    <a href="/process/login_page">BACK</a>
                </section>
            </form>
        </section>
    </section>

